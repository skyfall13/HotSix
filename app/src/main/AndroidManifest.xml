<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dusdj.termp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_hotsix"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.hotsix.www.hotsixpaymentmodule.PaymentSelectActivity" />
        <activity android:name="com.hotsix.www.hotsixpaymentmodule.PaymentActivity" />
        <activity android:name="com.hotsix.www.hotsixpaymentmodule.PaymentCardActivity" />
        <activity android:name="com.hotsix.www.hotsixpaymentmodule.PaymentPointSelectActivity" />
        <activity android:name="com.hotsix.www.hotsixpaymentmodule.PaymentAccountActivity" />

        <activity android:name="com.hotsix.www.hotsixshoppingcart_module.MealTicketViewAvtivity"/>
        <activity android:name="com.hotsix.www.hotsixshoppingcart_module.ShoppingCartActivity"/>

        <activity android:name=".MenuActivity" />

    </application>

</manifest>